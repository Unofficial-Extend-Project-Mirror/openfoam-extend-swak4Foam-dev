actions
(
    {
        name    porousCells;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (2.05 0.4 -1) (2.1 0.85 1);
    }
    {
        name    porousZone;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     porousCells;
    }
);

