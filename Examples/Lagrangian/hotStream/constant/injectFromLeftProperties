FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      kinematicCloud1Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solution
{
    active          yes;
    coupled         true;
    transient       yes;

    cellValueSourceCorrection on;

    sourceTerms
    {
        schemes
        {
            rho             explicit 1;
            U               explicit 1;
            Yi              explicit 1;
            hs              explicit 1;
        }
    }

    interpolationSchemes
    {
        rhoPseudo             cell;
        U               cellPoint;
        mu              cell;
        T               cell;
        Cp              cell;
        p               cell;
    }

    integrationSchemes
    {
        U               Euler;
        T               analytical;
    }
}

constantProperties
{
    parcelTypeId    2;

    rhoMin           1e-15;
    minParticleMass  1e-15;
    rho0             5000;
    T0             400;
    TMin             300;
    Cp0              900;
    epsilon0         1;
    f0               0.5;
    Pr               0.7;

    pMin  1e4;
    constantVolume 1;
    Tvap   450;
    Tbp   370;

    youngsModulus   1e9;
    poissonsRatio   0.35;
}


subModels
{
    injectionModel  patchInjection;

    dispersionModel none;

    // PatchInteractionModel Rebound;
    patchInteractionModel standardWallInteraction;

    heatTransferModel RanzMarshall;

    compositionModel singlePhaseMixture;

    // PhaseChangeModel none;
    phaseChangeModel liquidEvaporation;

    radiation       off;

    surfaceFilmModel none;

    particleForces
    {
        gravity         on;
        nonSphereDrag {
            phi 0.5;
        };
        // virtualMass     off;
        // pressureGradient off;
    }

    ConeInjectionCoeffs
    {
        massTotal        0.002;
        parcelBasisType mass;

        SOI             0.001;
        duration        0.5;
        position        ( 0.05 0.09 0.005 );
        direction       ( 0 -1 0 );
        parcelsPerSecond 10000;
        flowRateProfile  constant 0.00001;
        Umag            constant 5e-1;
        thetaInner      constant 0;
        thetaOuter      constant 30;

        sizeDistribution
        {
            type         uniform;
            //        pdfType         RosinRammler;
            RosinRammlerPDF
            {
                minValue        5e-05;
                maxValue        0.001;
                d                7.5e-05;
                n                0.5 ;
            }
            uniformDistribution
            {
                minValue 5e-5;
                maxValue 1e-3;
            }
        }
    }

    patchInjectionCoeffs
    {
        patchName left;
        U0              ( 1 0 0 );

        $ConeInjectionCoeffs;
    }

    standardWallInteractionCoeffs
    {
        type            rebound;
    }

    ReboundCoeffs
    {
        UFactor 1;
    }

    RanzMarshallCoeffs
    {
        BirdCorrection  off;
    }

    singlePhaseMixtureCoeffs
    {
        phases
            (
                liquid
                {
                    H2O 1;
                    // H2O 0.8;
                    //            N2 0.2;
                }
            );
    }

    liquidEvaporationCoeffs
    {
        //    enthalpyTransfer enthalpyDifference;
        enthalpyTransfer latentHeat;

        activeLiquids
            (
                H2O
            );
    }
}

cloudFunctions
{
    patchPostProcessing
    {
        maxStoredParcels 10000;
        patches ( right );
    }
}

