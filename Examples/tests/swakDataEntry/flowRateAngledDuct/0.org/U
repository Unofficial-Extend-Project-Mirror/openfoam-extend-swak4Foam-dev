FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    front
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    back
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    porosityWall
    {
        type            slip;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            flowRateInletVelocity;
        //        type            groovyFlowRateInletVelocity;
        flowRateExpression "t<1 ? 0.1*t : 0.1";
        flowRate        swak {
            expression $flowRateExpression;
            valueType patch;
            patchName inlet;
            independentVariableName t;
        };
        volumetricFlowRate $flowRate;
        //        flowRate        constant 0.1;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            fluxCorrectedVelocity; //inletOutlet;
        value           uniform (0 0 0);
        inletValue      uniform (0 0 0);
    }
}

// ************************************************************************* //
