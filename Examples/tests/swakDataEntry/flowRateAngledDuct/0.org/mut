FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      mut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            mutkWallFunction;
        value           uniform 0;
    }
    back
    {
        type            mutkWallFunction;
        value           uniform 0;
    }
    wall
    {
        type            mutkWallFunction;
        value           uniform 0;
    }
    porosityWall
    {
        type            mutkWallFunction;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
}


