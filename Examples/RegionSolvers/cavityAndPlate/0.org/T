FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    movingWall
    {
        type            fixedValue;
        value uniform   310;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    fixedWallsBottom
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     fluidThermo;
        kappa $kappaMethod;
        kappaName none;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
