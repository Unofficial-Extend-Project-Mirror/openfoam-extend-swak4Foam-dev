FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoPimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         200;

deltaT          0.01;

writeControl    runTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           0.3;

maxDeltaT       1;

libs (
    "libsimpleRegionSolvers.so"
    "libsimpleSwakFunctionObjects.so"
    "libswakFunctionObjects.so"
    //    "libswakTopoSources.so"
    "libswakSourceFields.so"
);

functions {
    solvePlateHole {
        type solidDisplacementRegionSolver;
        meshRegion plateHole;
    }
    displacement {
        type swakExpression;
        valueType internalField;
        region plateHole;
        verbose true;
        accumulations (
            min
            max
        );
        expression "D";
    }
    temperatureCavity {
        type swakExpression;
        valueType internalField;
        verbose true;
        accumulations (
            min
            weightedQuantile0.01
            weightedAverage
            weightedQuantile0.99
            max
        );
        expression "T";
    }
    temperaturePlate {
        $temperatureCavity;
        region plateHole;
    }
}
  // *************************************************************************
  // //
