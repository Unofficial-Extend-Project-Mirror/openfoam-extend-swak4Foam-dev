FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000765;

boundaryField
{
    movingWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    "fixedWall.*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    cavity_to_leftHeater {
        $fixedWallBottom;
    }

    frontAndBack
    {
        type            empty;
    }
}


