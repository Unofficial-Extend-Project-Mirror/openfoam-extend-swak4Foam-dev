FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    "fixedWall.*"
    {
        type            zeroGradient;
    }

    cavity_to_leftHeater {
        $fixedWallBottom;
    }

    frontAndBack
    {
        type            empty;
    }
}

