FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 292;

boundaryField
{
    inletfuel
    {
        type            fixedValue;
        value           uniform 292;
    }
    inletair
    {
        type            fixedValue;
        value           uniform 292;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 292;
        value           uniform 292;
    }
    axis
    {
        type            empty;
    }
    leftside
    {
        type            zeroGradient;
    }
    burnerwall
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     fluidThermo;
        kappa $kappaMethod;
        kappaName none;
    }
    burnertip
    {
        $burnerwall;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


