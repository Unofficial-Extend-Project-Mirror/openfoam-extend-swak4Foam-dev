FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    T
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0;
    }
    TSteady
    {
        $T;
    }
    TSteadyAlternate
    {
        $T;
    }
    TTransient
    {
        $T;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 2;
}

TSteadyLaplacianPDE
{
    $SIMPLE;
    nCorrectors 10;
}

TSteadyAlternateLaplacianPDE
{
    $SIMPLE;
    nCorrectors 2;
}

TTransientLaplacianPDE
{
    $SIMPLE;
}

