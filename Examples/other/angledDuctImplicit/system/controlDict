    surfacePlane
    {
        type swakExpression;
        valueType surface;
        surfaceName testPlane;
        surface {
            type plane;
            basePoint       (0.0001 0.0001 0.0001);
            normalVector    (0 0 1);
            interpolate false;
        }
        verbose true;
        expression "mag(U)";
        accumulations (
            min
            max
        );
    }
